{
  "name": "devsign-server",
  "version": "1.0.0",
  "description": "backend for devsign",
  "main": "server.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "jest --verbose --runInBand --detectOpenHandles --passWithNoTests",
    "test:watch": "npm run test -- --watch",
    "start": "node server.js",
    "start:watch": "nodemon server.js",
    "seed": "node seed.js",
    "drop": "mongo tweets --eval 'db.dropDatabase()'",
    "db-load-all": "npm run drop && npm run seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caripizza/devsign-server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/caripizza/devsign-server/issues"
  },
  "homepage": "https://github.com/caripizza/devsign-server#readme",
  "dependencies": {
    "auth0": "^2.15.0",
    "chance": "^1.0.18",
    "express": "^4.16.4",
    "mongoose": "^5.4.19",
    "morgan": "^1.9.1",
    "dotenv": "^7.0.0"
  },
  "devDependencies": {
    "eslint": "^5.15.1",
    "express-jwt": "^5.3.1",
    "jest": "^24.5.0",
    "jwks-rsa": "^1.4.0",
    "nodemon": "^1.18.10",
    "supertest": "^4.0.2"
  }
}
